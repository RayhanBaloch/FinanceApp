<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <Color x:Key="BackgroundLight">#efefef</Color>
    <SolidColorBrush x:Key="BackgroundLightBrush" Color="{StaticResource BackgroundLight}" />

    <Color x:Key="BackgroundVeryLight">#fafafa</Color>
    <SolidColorBrush x:Key="BackgroundVeryLightBrush" Color="{StaticResource BackgroundVeryLight}" />

    <Color x:Key="BackgroundDark">#bdbdbd</Color>
    <SolidColorBrush x:Key="BackgroundDarkBrush" Color="{StaticResource BackgroundDark}" />

    <Color x:Key="ForegroundMain">#686868</Color>
    <SolidColorBrush x:Key="ForegroundMainBrush" Color="{StaticResource ForegroundMain}" />

    <Color x:Key="ForegroundVeryDark">#000000</Color>
    <SolidColorBrush x:Key="ForegroundVeryDarkBrush" Color="{StaticResource ForegroundVeryDark}" />

    <Color x:Key="ForegroundLight">#ffffff</Color>
    <SolidColorBrush x:Key="ForegroundLightBrush" Color="{StaticResource ForegroundLight}" />

    <Color x:Key="ForegroundDark">#bdbdbd</Color>
    <SolidColorBrush x:Key="ForegroundDarkBrush" Color="{StaticResource ForegroundDark}" />

    <Color x:Key="AppBlue">#3099c5</Color>
    <SolidColorBrush x:Key="AppBlueBrush" Color="{StaticResource AppBlue}" />

    <Color x:Key="AppTransparentBlue">#003099c5</Color>
    <SolidColorBrush x:Key="AppTransparentBlueBrush" Color="{StaticResource AppTransparentBlue}" />

    <Color x:Key="AppLightBlue">#45b6e5</Color>
    <SolidColorBrush x:Key="AppLightBlueBrush" Color="{StaticResource AppLightBlue}" />

    <Color x:Key="AppVeryLightBlue">#c3e5f3</Color>
    <SolidColorBrush x:Key="AppVeryLightBlueBrush" Color="{StaticResource AppVeryLightBlue}" />

    <Color x:Key="AppVeryVeryLightBlue">#ecf6fb</Color>
    <SolidColorBrush x:Key="AppVeryVeryLightBlueBrush" Color="{StaticResource AppVeryVeryLightBlue}" />

    <Color x:Key="AppDarkBlue">#0c6991</Color>
    <SolidColorBrush x:Key="AppDarkBlueBrush" Color="{StaticResource AppDarkBlue}" />

    <Color x:Key="AppOrange">#ffa800</Color>
    <SolidColorBrush x:Key="AppOrangeBrush" Color="{StaticResource AppOrange}" />

    <Color x:Key="AppRed">#ff4747</Color>
    <SolidColorBrush x:Key="AppRedBrush" Color="{StaticResource AppRed}" />

    <Color x:Key="AppGreen">#00c541</Color>
    <SolidColorBrush x:Key="AppGreenBrush" Color="{StaticResource AppGreen}" />

    <Color x:Key="AppCyanBlue">#2E333A</Color>
    <SolidColorBrush x:Key="AppCyanBlueBrush" Color="{StaticResource AppCyanBlue}" />

</ResourceDictionary>